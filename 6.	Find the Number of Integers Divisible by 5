#include <stdio.h>

int main() {
    int lowerLimit, upperLimit;

    // Input the lower and upper limits
    printf("Enter the lower limit: ");
    scanf("%d", &lowerLimit);

    printf("Enter the upper limit: ");
    scanf("%d", &upperLimit);

    // Calculate the number of integers divisible by 5
    int divisibleBy5 = (upperLimit / 5) - ((lowerLimit - 1) / 5);

    // Output the result
    printf("Number of integers divisible by 5 between %d and %d is: %d\n", lowerLimit, upperLimit, divisibleBy5);

    return 0;
}
