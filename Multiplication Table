#include <stdio.h>

int main() {
    // Declare variables
    int number;

    // Input the number
    printf("Enter an integer to generate its multiplication table: ");
    scanf("%d", &number);

    // Display the multiplication table
    printf("Multiplication table of %d:\n", number);

    for (int i = 1; i <= 10; ++i) {
        printf("%d x %d = %d\n", number, i, number * i);
    }

    return 0;
}
